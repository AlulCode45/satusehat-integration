{
    "name": "ivanwilliammd/satusehat-integration",
    "description": "Build SATUSEHAT FHIR Object in Easy Way",
    "keywords": [
        "ivanwilliammd",
        "satusehat-integration"
    ],
    "homepage": "https://github.com/ivanwilliammd/satusehat-integration",
    "license": "MIT",
    "authors": [
        {
            "name": "ivanwilliammd",
            "email": "ivanwilliam.md@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Ivanwilliammd\\SatusehatIntegration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivanwilliammd\\SatusehatIntegration\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
